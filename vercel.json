{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/enviar-para-sheets.js" },
    { "source": "/(.*)", "destination": "/$1" }
  ],
  "functions": {
    "api/enviar-para-sheets.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "18.x"
    }
  }
}

